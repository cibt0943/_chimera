{
  "name": "chimera",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development --progress",
    "build:development": "webpack --mode=development --progress",
    "build:production": "webpack --mode=production --progress",
    "fix": "npm run -s format && npm run -s lint:fix",
    "format": "prettier --write --loglevel=warn './frontend/**/*.{js,jsx,ts,tsx,json}'",
    "lint": "npm run -s lint:style && npm run -s lint:es",
    "lint:fix": "npm run -s lint:style:fix && npm run -s lint:es:fix",
    "lint:es": "eslint './frontend/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix './frontend/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint './frontend/**/*.{css,scss}'",
    "lint:style:fix": "stylelint --fix './frontend/**/*.{css,scss}'",
    "lint-staged": "lint-staged --no-stash",
    "postinstall": "typesync"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./pre-commit.sh"
    }
  },
  "lint-staged": {
    "./frontend/**/*.{ts,tsx}": [
      "prettier --loglevel=warn",
      "eslint"
    ],
    "./frontend/**/*.{css,scss}": [
      "stylelint"
    ],
    "*.{rb,rake}": [
      "bundle exec rubocop"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.13.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.7",
    "@types/file-loader": "^4.2.1",
    "@types/mini-css-extract-plugin": "^1.4.0",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.2",
    "@types/sass": "^1.16.0",
    "@types/stylelint": "^9.10.1",
    "@types/webpack": "^5.0.0",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-manifest-plugin": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@welldone-software/why-did-you-render": "^6.1.1",
    "autoprefixer": "^10.2.5",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.0.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^1.4.0",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.0.4",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.12.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "typesync": "^0.8.0",
    "url-loader": "^4.1.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^3.1.0"
  }
}
