{
  "name": "chimera",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development --progress",
    "build:development": "webpack --mode=development --progress",
    "build:production": "webpack --mode=production --progress",
    "fix": "npm run -s format && npm run -s lint:fix",
    "format": "prettier --write --loglevel=warn './frontend/**/*.{js,jsx,ts,tsx,json}'",
    "lint": "npm run -s lint:style && npm run -s lint:es",
    "lint:fix": "npm run -s lint:style:fix && npm run -s lint:es:fix",
    "lint:es": "eslint './frontend/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix './frontend/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint './frontend/**/*.{css,scss}'",
    "lint:style:fix": "stylelint --fix './frontend/**/*.{css,scss}'",
    "lint-staged": "lint-staged --no-stash",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "typesync"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./pre-commit.sh"
    }
  },
  "lint-staged": {
    "./frontend/**/*.{ts,tsx}": [
      "prettier --loglevel=warn",
      "eslint"
    ],
    "./frontend/**/*.{css,scss}": [
      "stylelint"
    ],
    "*.{rb,rake}": [
      "bundle exec rubocop"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.12.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/autoprefixer": "^9.7.2",
    "@types/babel__core": "^7.1.12",
    "@types/eslint": "^7.2.6",
    "@types/file-loader": "^4.2.0",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node-sass": "^4.11.1",
    "@types/prettier": "^2.2.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-is": "^17.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.2",
    "@types/stylelint": "^9.10.1",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-manifest-plugin": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@welldone-software/why-did-you-render": "^6.1.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.0.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.11.0",
    "postcss-loader": "^4.0.1",
    "prettier": "^2.0.4",
    "react-is": "^17.0.1",
    "redux-devtools-extension": "^2.13.9",
    "sass-loader": "^10.0.2",
    "stylelint": "^13.5.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.12.1",
    "ts-loader": "^8.0.1",
    "typescript": "^4.2.3",
    "typesync": "^0.8.0",
    "url-loader": "^4.1.0",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^3.0.0"
  }
}
